{
  "name": "gym-buddy-api",
  "version": "0.0.1",
  "description": "Gym buddy is a mobile application created as an university project.",
  "author": "Paulo Vareiro, Diogo Bernardes, Sergio Barbosa",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "vercel-build": "sh scripts/deploy.sh",
    "build": "tsc --project tsconfig.json",
    "build:master": "prisma migrate deploy && tsc --project tsconfig.json",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "postinstall": "prisma generate"
  },
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.2",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  }
}
